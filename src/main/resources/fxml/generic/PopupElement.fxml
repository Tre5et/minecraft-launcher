<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="rootPane" visible="false" stylesheets="@/css/generic/PopupElement.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.treset.minecraftlauncher.ui.generic.PopupElement">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" />
        <ColumnConstraints hgrow="SOMETIMES" />
        <ColumnConstraints hgrow="ALWAYS" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="ALWAYS" />
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints vgrow="ALWAYS" />
    </rowConstraints>
    <GridPane styleClass="popup-container" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints valignment="TOP" vgrow="SOMETIMES" />
            <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
        </rowConstraints>
        <VBox alignment="TOP_CENTER" spacing="10.0">
            <Label fx:id="titleLabel" styleClass="title" text="You should not be seeing this" />
            <Label fx:id="messageLabel" styleClass="message" text="This isn't good" />
        </VBox>
        <HBox fx:id="buttonContainer" alignment="CENTER" spacing="10.0" GridPane.rowIndex="1">
        </HBox>
    </GridPane>
</GridPane>
