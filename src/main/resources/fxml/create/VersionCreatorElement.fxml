<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="rootBox" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.treset.minecraftlauncher.ui.create.VersionCreatorElement">
   <ComboBox fx:id="cbVersion" promptText="%creator.version.prompt.version" />
   <CheckBox fx:id="chSnapshots" mnemonicParsing="false" onAction="#onCheckSnapshots" text="%creator.version.checkbox.snapshots" />
   <Label fx:id="lbVersionError" styleClass="error" text="%creator.version.label.error.version" visible="false" />
   <ComboBox fx:id="cbType" promptText="%creator.version.prompt.type" />
   <Label fx:id="lbTypeError" styleClass="error" text="%creator.version.label.error.type" visible="false" />
   <ComboBox fx:id="cbLoader" promptText="%creator.version.prompt.loaderversion" />
   <Label fx:id="lbLoaderError" styleClass="error" text="%creator.version.label.error.loader" visible="false" />
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
